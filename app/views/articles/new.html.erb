<h1>新增姓名的页面</h1>

<%= form_for :article , url: articles_path do |f| %>
  <p>
    <%= f.label :姓名 %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :字号 %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.submit :保存%>
  </p>

  <%= link_to '返回',articles_path %>
<% end %>



